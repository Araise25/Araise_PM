{
    "packages": {
      "extensions": [
        {
          "name": "ysb",
          "version": "1.0.0",
          "description": "YouTube Sidebar - A browser extension to enhance YouTube experience",
          "type": "extension",
          "browsers": {
            "firefox": {
              "repo": "https://github.com/Araise25/YSB",
              "path": ".",
              "link": "https://addons.mozilla.org/en-US/developers/addon/ysb/"
            }
          },
          "author": "Araise25",
          "license": "MIT",
          "tags": ["youtube", "browser-extension", "productivity"]
        }
      ],
      "scripts": [
        {
          "name": "Kill Port",
          "type": "script",
          "description": "Port Killer - A utility script to kill processes running on specific ports",
          "aliases": ["killport"],
          "repo": "https://github.com/Araise25/test_scripts.git",
          "path": ".",
          "main_script": "killport.sh",
          "run_command": "bash killport.sh",
          "author": "Araise25",
          "license": "MIT",
          "tags": ["utility", "system", "port-management"],
          "dependencies": {
            "required": ["bash"],
            "optional": []
          }
        },
        {
          "name": "Test Script",
          "type": "script",
          "description": "Test Script - A utility script for testing purposes",
          "aliases": ["test"],
          "repo": "https://github.com/Araise25/test_scripts.git",
          "path": ".",
          "main_script": "test.sh",
          "run_command": "bash test.sh",
          "author": "Araise25",
          "license": "MIT",
          "tags": ["utility", "testing"],
          "dependencies": {
            "required": ["bash"],
            "optional": []
          }
        }
      ],
      "apps": []
    }
  }
